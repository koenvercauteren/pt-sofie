{
  "name": "pt-sofie",
  "description": "Personal Trainer - Sofie de Backer",
  "version": "1.0.0",
  "author": "Koen Vercauteren <knvercauteren@gmail.com>",
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "animate.css": "^3.7.0",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-material-ui": "^0.1.3",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sitemap": "^1.2.25",
    "intl": "^1.2.5",
    "react-animate-on-scroll": "^2.1.5",
    "react-google-recaptcha": "^1.0.5",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-reveal": "^1.2.2",
    "react-scrollable-anchor": "^0.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^2.0.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.2.0",
    "prettier": "^1.12.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> env.production && gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js' --fix",
    "test": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koenvercauteren/pt-sofie"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|png)$": "identity-obj-proxy",
      "^.+\\.(svg)$": "<rootDir>/test/svgImportMock.js"
    },
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/.cache"
    ]
  }
}
